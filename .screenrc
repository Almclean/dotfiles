# Term
#
term xterm-256color

# Encoding
encoding UTF-8

vbell off
startup_message off
hardstatus alwayslastline
hardstatus string '%{= dW}%-Lw%{= dY}%50> %n*%f %t%{= dW}%+Lw%< %{= dW}%-=%D %m/%d/%y %C %A %1`%{-}'
defscrollback 5000
altscreen on
attrcolor b ".I"
defbce "on"
sorendition dY
shell -$SHELL

